{
  "extends": "astro/tsconfigs/base",
  "compilerOptions": {
    "allowSyntheticDefaultImports": true,
    "strictNullChecks": true,
    "allowJs": true,
    "baseUrl": ".",
    "paths": {
      "@components/*": [
        "src/components/*"
      ],
      "@assets/*": [
        "src/assets/*"
      ],
      "@/*": [
        "src/*"
      ]
    },
    "types": [
      "vue"
    ],
  },
  "include": [
    "src/**/*"
  ]
}