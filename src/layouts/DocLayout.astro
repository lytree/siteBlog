---
import DocHeader from '@/components/DocHeader.astro';
import BaseLayout from './BaseLayout.astro';
import PageFrame from '@/components/PageFrame.astro';
interface Props {
  title: string;
  description?: string;
  hasSidebar?: boolean;
}
---

<BaseLayout title={Astro.props.title} description={Astro.props.title}>
  <PageFrame {...Astro.props}> <DocHeader slot="header" {...Astro.props} /><slot /></PageFrame>
</BaseLayout>
