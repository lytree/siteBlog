  // scss-docs-end progress-variables

  // List group

  // scss-docs-start list-group-variables
  $list-group-color: var(--body-color);
  $list-group-bg: var(--body-bg);
  $list-group-border-color: var(--border-color);
  $list-group-border-width: var(--border-width);
  $list-group-border-radius: var(--border-radius);

  $list-group-item-padding-y: $spacer * 0.5;
  $list-group-item-padding-x: $spacer;
  // fusv-disable
  $list-group-item-bg-scale: -80%; // Deprecated in v5.3.0
  $list-group-item-color-scale: 40%; // Deprecated in v5.3.0
  // fusv-enable

  $list-group-hover-bg: var(--tertiary-bg);
  $list-group-active-color: $component-active-color;
  $list-group-active-bg: $component-active-bg;
  $list-group-active-border-color: $list-group-active-bg;

  $list-group-disabled-color: var(--secondary-color);
  $list-group-disabled-bg: $list-group-bg;

  $list-group-action-color: var(--secondary-color);
  $list-group-action-hover-color: var(--emphasis-color);

  $list-group-action-active-color: var(--body-color);
  $list-group-action-active-bg: var(--secondary-bg);
  // scss-docs-end list-group-variables

  // Image thumbnails

  // scss-docs-start thumbnail-variables
  $thumbnail-padding: 0.25rem;
  $thumbnail-bg: var(--body-bg);
  $thumbnail-border-width: var(--border-width);
  $thumbnail-border-color: var(--border-color);
  $thumbnail-border-radius: var(--border-radius);
  $thumbnail-box-shadow: var(--box-shadow-sm);
  // scss-docs-end thumbnail-variables

  // Figures

  // scss-docs-start figure-variables
  $figure-caption-font-size: $small-font-size;
