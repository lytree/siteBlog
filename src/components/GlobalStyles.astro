  // scss-docs-start breadcrumb-variables
  $breadcrumb-font-size: null;
  $breadcrumb-padding-y: 0;
  $breadcrumb-padding-x: 0;
  $breadcrumb-item-padding-x: 0.5rem;
  $breadcrumb-margin-bottom: 1rem;
  $breadcrumb-bg: null;
