  // Tooltips

  // scss-docs-start tooltip-variables
  $tooltip-font-size: $font-size-sm;
  $tooltip-max-width: 200px;
  $tooltip-color: var(--body-bg);
  $tooltip-bg: var(--emphasis-color);
  $tooltip-border-radius: var(--border-radius);
