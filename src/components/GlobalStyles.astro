  $popover-arrow-outer-color: var(--border-color-translucent);
  // fusv-enable

  // Toasts

  // scss-docs-start toast-variables
  $toast-max-width: 350px;
  $toast-padding-x: 0.75rem;
  $toast-padding-y: 0.5rem;
  $toast-font-size: 0.875rem;
  $toast-color: null;
